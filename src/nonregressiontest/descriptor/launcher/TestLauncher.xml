<?xml version="1.0" encoding="UTF-8"?>
<ProActiveDescriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../descriptors/DescriptorSchema.xsd">
	<mainDefinition id="main" class="nonregressiontest.descriptor.launcher.A">
		<mapToVirtualNode value="VNmain"/>
	</mainDefinition>
	<componentDefinition>
		<virtualNodesDefinition>
			<virtualNode name="VNmain"/>
			<virtualNode name="VN1"/>
			<virtualNode name="VN2"/>
		</virtualNodesDefinition>
	</componentDefinition>
	<deployment>
		<mapping>
			<map virtualNode="VNmain">
				<jvmSet>
					<vmName value="JvmMain"/>
				</jvmSet>
			</map>
			<map virtualNode="VN1">
				<jvmSet>
					<vmName value="Jvm1"/>
				</jvmSet>
			</map>
			<map virtualNode="VN2">
				<jvmSet>
					<vmName value="Jvm2"/>
				</jvmSet>
			</map>						
		</mapping>
		<jvms>
			<jvm name="JvmMain">
				<creation>
					<processReference refid="localJVM"/>
				</creation>
			</jvm>
			<jvm name="Jvm1">
				<creation>
					<processReference refid="localJVM"/>
				</creation>
			</jvm>
			<jvm name="Jvm2">
				<creation>
					<processReference refid="localJVM"/>
				</creation>
			</jvm>
		</jvms>
	</deployment>
	<infrastructure>
		<processes>
			<processDefinition id="localJVM">
				<jvmProcess class="org.objectweb.proactive.core.process.JVMNodeProcess">
				</jvmProcess>
			</processDefinition>
			
		</processes>
	</infrastructure>
</ProActiveDescriptor>
